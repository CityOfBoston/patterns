    box-sizing: border-box;
    padding: 0;
    display: none
}
.form-type-radio label:before,
.form-type-checkbox label:before,
.form-type-bef-checkbox label:before {
    content: "";
    display: inline-block;
    width: 39px;
    height: 39px;
    border: 3px solid $charles-blue;
    float: left;
    margin-right: 1em
}
.form-type-bef-checkbox label:before {
    margin-top: 34px;
    -ms-transform: translateY(-100%);
    transform: translateY(-100%)
}
.form-type-radio.error label:before,
.form-type-checkbox.error label:before,
.form-type-bef-checkbox.error label:before {
    border-color: $freedom-red;
    background-color: transparent
}
.form-type-radio.disabled label:before,
.form-type-checkbox.disabled label:before,
.form-type-bef-checkbox.disabled label:before {
    border-color: $grey-300;
    background-color: $grey-000
}
.form-type-radio label:before {
    border-radius: 100%
}
input[type=radio]:checked+label:before {
    content: "";
    color: $optimistic-blue;
    background: url("../images/global/icons/radio_fill.png") no-repeat center center transparent
}
input[type=checkbox]:checked+label:before {
    content: "";
    color: $optimistic-blue;
    background: url("/images/public/icons/check.svg") no-repeat center center transparent
}
input[type=radio].disabled+label:before,
input[type=checkbox].disabled+label:before {
    border-color: $grey-300;
    background-color: $grey-000
}
input[type=radio].error+label:before,
input[type=checkbox].error+label:before {
    border-color: $freedom-red;

.hb {
    top: -3px;
    position: relative;
    padding: 0 0;
    display: inline-block;
    cursor: pointer;
    transition-property: margin, opacity, filter;
    transition-duration: 0.15s;
    transition-timing-function: linear;
    font: inherit;
    color: inherit;
    text-transform: none;
    background-color: transparent;
    border: 0;
    margin: 10px 0 0;
    overflow: visible;
    outline: none

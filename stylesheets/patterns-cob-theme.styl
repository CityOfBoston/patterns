}
.dr .squiggle-border-bottom .supporting-text {
    padding-bottom: 3rem
}
.dr .squiggle-border-bottom {
    background: none;
    background-position: left top;  
}
.supporting-text {
    font-weight: 400;
    font-style: italic;
    line-height: inherit;

@require '../stylesheets/variables/_colors.styl';

body {
    overflow-x: hidden
}
html {
    font-size: 112.5%;
    line-height: 2.16667em;
    font-family: "Montserrat", sans-serif;
    -ms-text-size-adjust: 100%;
    -webkit-text-size-adjust: 100%;
    min-height: 100%;
    scroll-padding-top: 5.625rem
}
@media print {
    html {
        font-size: 12pt
    }
}

* {
    box-sizing: border-box
}
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
main,
menu,
nav,
section,
summary {
    display: block
}
audio,
canvas,
progress,
video {
    display: inline-block;
    vertical-align: baseline
}
audio:not([controls]) {
    display: none;
    height: 0
}
[hidden] {
    display: none
}
template {
    display: none
}
.hidden,
html.js .js-hidden,
html.js .element-hidden,
html.js .js-hide {
    display: none
}
.clearfix:before,
.cabinet .node-person-profile.view-mode-listing:before,
.views-exposed-form .views-exposed-widget:before,
.footer:before,
.tabs:before,
.tabs--secondary:before {
    content: '';
    display: table;
}
.clearfix:after,
.cabinet .node-person-profile.view-mode-listing:after,
.views-exposed-form .views-exposed-widget:after,
.footer:after,
.tabs:after,
.tabs--secondary:after {
    content: '';
    display: table;
    clear: both
}
img {
    border: 0
}
.media img {
    max-width: 100%;
    width: auto;
    height: auto
}
svg:not(:root) {
    overflow: hidden
}
:link {
    color: $optimistic-blue-dark;
    text-decoration: none
}
.inverted,
.inverted:link {
    color: $freedom-red-dark
}
:visited {
    color: $optimistic-blue-dark
}
.inverted:visited {
    color: $freedom-red-dark
}
a:hover,
a:focus {
    color: $freedom-red-dark
}
a.inverted:hover,
a.inverted:focus {
    color: $optimistic-blue-dark
}
a:active {
    color: $freedom-red-dark;
    background-color: transparent
}
a.inverted:active {
    color: $optimistic-blue-dark
}
a:active,
a:hover {
    outline: 0
}
@media print {
    :link,
    :visited {
        text-decoration: underline
    }
    a[href]:after {
        content: " (" attr(href) ")";
        font-weight: normal;
        font-size: 18px;
        text-decoration: none
    }
    a[href^='javascript:']:after,
    a[href^='#']:after {
        content: ''
    }
}
.button {
    font-size: 14px;
    padding: 1rem
}
.divider,
hr {
    margin: 39px 0px;
    margin: 2.16667rem 0rem;
    border: 0;
    border-top: 1px solid $grey-100
}
.divider>:first-child,
hr>:first-child {
    margin-top: 39px;
    margin-top: 2.16667rem
}
blockquote {
    margin: 39px 36px;
    margin: 2.16667rem 2rem;
    color: $charles-blue;
    border-left: 3px solid $freedom-red-dark;
    font-style: italic;
    padding-left: 2em;
    font-size: 18px;
    line-height: 38px;
    font-size: 1rem;
    line-height: 2.11111rem
}
blockquote p {
    margin-top: 0;
    margin-bottom: 1rem
}
blockquote footer {
    color: $body-text;
    font-size: 14px;
    line-height: 22px;
    font-size: 0.77778rem;
    line-height: 1.22222rem;
    font-family: "Montserrat", sans-serif;
    text-transform: uppercase;
    font-style: normal
}
blockquote footer:before {
    content: "- "
}
address {
    text-indent: 0;
    text-transform: uppercase;
    font-family: "Montserrat", sans-serif;
    color: $charles-blue;
    margin-top: 2.16667rem;
    margin-bottom: .475rem;
    font-size: 0.88889rem;
    line-height: 1.22222rem;
    letter-spacing: 1px;
    font-style: normal
}
figure {
    margin: 0px 0px 39px;
    margin: 0rem 0rem 2.16667rem
}
hr {
    box-sizing: content-box;
    height: 0
}
p,
pre {
    margin: 8.97px 0px 20px;
    margin: 0.49833rem 0rem 1.16667rem
}
pre,
code,
kbd,
samp,
var {
    font-family: Menlo, "DejaVu Sans Mono", "Ubuntu Mono", Courier, "Courier New", monospace, sans-serif;
    font-size: 1em
}
pre {
    overflow: auto
}
rticle dl,
article menu,
article ol,
article ul {
    margin: 8.97px 0px 39px;
    margin: 0.49833rem 0rem 2.16667rem
}
article ol ol,
article ol ul,
article ul ol,
article ul ul {
    margin: 0
}
article dd {
    margin: 0 0 0 36px
}
[dir="rtl"] article dd {
    margin: 0 36px 0 0
}
article menu,
article ol,
article ul {
    padding: 0 0 0 0;
    list-style-position: inside
}
[dir="rtl"] article menu,
[dir="rtl"] article ol,
[dir="rtl"] article ul {
    padding: 0 0 0 0
}
article ol {
    counter-reset: section;
    list-style-type: none;
    color: $charles-blue
}
article ol>li:before {
    border: 3px solid;
    border-radius: 100%;
    counter-increment: section;
    content: counters(section, ".") " ";
    display: inline-block;
    height: 40px;
    line-height: 40px;
    margin: 9.75px 0px 0px 0px;
    margin: 0.54167rem 0rem 0rem 0rem;
    text-align: center;
    width: 40px;
    font-family: "Montserrat", sans-serif;
    text-indent: 0;
    margin-left: -55px;
    margin-right: 5px
}
table {
    margin: 39px 0px;
    margin: 2.16667rem 0rem;
    border-collapse: collapse;
    border-spacing: 0;
    width: 100%
}
td,
th {
    padding: 0
}
abbr[title] {
    border-bottom: 1px dotted
}
@media print {
    abbr[title]:after {
        content: " (" attr(title) ")"
    }
}
b,
strong {
    font-weight: bold
}
dfn {
    font-style: italic
}
mark {
    background: #fd0;
    color: $body-text
}
small {
    font-size: 16px;
    font-size: 0.88889rem
}
sub,
sup {
    font-size: 14px;
    font-size: 0.77778rem;
    line-height: 0;
    position: relative;
    vertical-align: baseline
}
sup {
    top: -.5em
}
sub {
    bottom: -.25em
}
.container,
.component__content,
.component__header,
.content {
    max-width: 66.667rem;
    margin-left: auto;
    margin-right: auto;
    padding-left: 10%;
    padding-right: 10%
}
.container:after,
.component__content:after,
.component__header:after,
.content {
    content: " ";
    display: block;
    clear: both
}
@media (min-width: 1300px) {
    .container,
    .component__content,
    .component__header,
    .content {
        padding-left: 0;
        padding-right: 0
    }
}
.button-icon {
    padding-right: 1.5em;
    position: relative
}
.button-icon:after {
    content: "";
    display: inline-block;
    width: 26px;
    height: 26px;
    background-image: url("../images/global/icons/document-icon.png");
    background-position: center center;
    background-repeat: no-repeat;
    margin-left: 1.5em;
    margin-bottom: -5px
}
button,
input,
optgroup,
select,
textarea {
    box-sizing: border-box;
    max-width: 100%;
    color: inherit;
    font: inherit;
    margin: 0
}
button {
    overflow: visible
}
button,
select {
    text-transform: none
}
button::-moz-focus-inner,
input::-moz-focus-inner {
    border: 0;
    padding: 0
}
button,
input,
optgroup,
select,
textarea {
    box-sizing: border-box;
    max-width: 100%;
    color: inherit;
    font: inherit;
    margin: 0
}
input {
    color: $charles-blue;
    border: 3px solid;
    padding: 5px 15px;
    height: 65px
}
input::-webkit-input-placeholder {
    color: $grey-300;
    font-style: italic;
    line-height: 1
}
input:-moz-placeholder {
    color: $grey-300;
    font-style: italic;
    line-height: 1
}
input::-moz-placeholder {
    color: $grey-300;
    font-style: italic;
    line-height: 1
}
input:-ms-input-placeholder {
    color: $grey-300;
    font-style: italic;
    line-height: 1
}
input.error {
    border-color: $freedom-red-dark
}
.input-error-mssg {
    color: $freedom-red-dark
}
html input[type="button"],
[type="reset"],
[type="submit"],
button {
    -webkit-appearance: button;
    border: 0
}
input[type="number"]::-webkit-inner-spin-button,
input[type="number"]::-webkit-outer-spin-button {
    height: auto
}
input[type="search"] {
    -webkit-appearance: textfield
}
input[type="search"]::-webkit-search-cancel-button,
input[type="search"]::-webkit-search-decoration {
    -webkit-appearance: none
}
input[type="text"],
input[type="search"],
input[type="tel"],
input[type="url"],
input[type="email"],
input[type="password"],
input[type="date"],
input[type="time"],
input[type="number"],
textarea {
    line-height: inherit
}
input[type="checkbox"],
input[type="radio"] {
    box-sizing: border-box;
    padding: 0;
    display: none
}
fieldset {
    padding: 13.65px 24.375px 25.35px;
    padding: 0.75833rem 1.35417rem 1.40833rem;
    border: 1px solid $grey-100;
    margin: 0 2px
}
legend {
    border: 0;
    margin-left: -5px;
    padding: 0 5px
}
label {
    display: block;
    color: $charles-blue
}
label {
    font-family: "Montserrat", sans-serif;
    text-transform: uppercase;
    font-weight: 400;
    font-size: responsive 13px 14px;
    line-height: 3.9px;
    font-size: 0.75rem;
    line-height: 0.21667rem;
    margin: 29.25px 0px 9.75px 0px;
    margin: 1.625rem 0rem 0.54167rem 0rem;
    letter-spacing: 1px
}
.squiggle-border-top {
    background: url("../images/squiggle.svg") repeat-x center top transparent;
    background-size: 383px 30px
}
.squiggle-border-bottom,
.paragraphs-item-text .entity-paragraphs-item,

<footer class="ft">
  <div class="ft-c ft-ite">
    <ul class="ft-ll ft-ite-links">
      <li class="ft-ll-i">
        <a href="http://www.cityofboston.gov/311/" class="ft-ll-a lnk--yellow">
          {{ bos311.name }} - {{ bos311.slogan }}
        </a>
      </li>
      {{#each links as |link|}}
        <li class="ft-ll-i"><a href="{{ link.href }}" class="ft-ll-a">{{ link.name }}</a></li>
      {{/each}}
    </ul>

    <ul class="ft-ll ft-ite-311">
      <li class="ft-ll-i">
        <a href="http://www.cityofboston.gov/311/" class="ft-ll-a lnk--yellow">
          <span class="ft-ll-311">{{ bos311.name }}</span>
          <span class="tablet--hidden"> - </span>
          {{ bos311.slogan }}
        </a>
      </li>
    </ul>
  </div>
</footer>

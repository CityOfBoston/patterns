  <div class="contact-title">Contact</div>
  {{/if}}

  {{#if text-header}}
  <div class="date-title t--intro">{{text-header}}</div>
  {{/if}}
  <ul class="sb">

    {{#each icon-items as |item|}}
    <li class="sb-i evt-sb-i">
      {{#if item.icon}}<img class="sb-ic" src="{{ item.icon }}"></img> {{/if}}
      <span class="sb-d"> {{#if item.description.href}} <a href="{{item.description.href}}">{{
          item.description.text}}</a> {{else}} {{ item.description.text}} {{/if}}</span>
      <div class="sb-b"> {{#if item.body}} {{item.body}} {{/if}} </div>
    </li>
    {{/each}}

    {{#if title-items}}
    {{#each title-items as |detail|}}
    <li class="dl-i">
      <span class="dl-t">{{ detail.name }}</span>
      <span class="dl-d" style="padding-left: 70px;">{{ detail.description }}</span>
    </li>
    {{/each}}
    {{/if}}

    {{#if list-items}}
    <li class="sb-i evt-sb-i">
      {{#if list-items-title}}<div class="sb-h"> {{list-items-title}}</div> {{/if}}
      {{#each list-items as |item|}}
      <ul class="ul list-item">
        <li><a href="{{item.href}}" class="t--info sb-l">{{item.text}}</a>
          {{item.detail}}
        </li>
      </ul>
      {{/each}}
    </li>
    {{/if}}

    <li class="sb-i evt-sb-i">
      {{#if resource-items-title}}<div class="sb-h"> {{resource-items-title}}</div> {{/if}}
      {{#each resource-items as |item|}}
      {{> @list }}
      {{/each}}
    </li>
  </ul>

  {{#if social-icons}}
  <div class="list-item">
    <div class="detail-item" bos_context_type="Social Media Links">
      <div class="detail-item__content">
        <div class="detail-item__body">
          {{#if social-icons.twitter}}
          <a href="https://twitter.com/bostonlandmarks" class="icon-twitter icon-social" target="_blank"
            bos_context_type="Social Networking">
            icon-twitter
          </a>
          {{/if}}
          {{#if social-icons.facebook}}
          <a href="https://www.facebook.com/BostonLandmarks" class="icon-facebook icon-social" target="_blank"
            bos_context_type="Social Networking">
            icon-facebook
          </a>
          {{/if}}
          {{#if social-icons.instagram}}
          <a href="https://www.instagram.com/bostonlandmarks/" class="icon-instagram icon-social" target="_blank"
            bos_context_type="Social Networking">
            icon-instagram
          </a>
          {{/if}}
          {{#if social-icons.flickr}}
          <a href="https://www.flickr.com/photos/bostonlandmarkscommission/" class="icon-flickr icon-social"
            target="_blank" bos_context_type="Social Networking">
            icon-flickr
          </a>
          {{/if}}
        </div>
      </div>
    </div>
  </div>
  {{/if}}

</div>